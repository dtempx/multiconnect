{
  "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "removeComments": true,
      "declaration": true,
      "resolveJsonModule": true,
      "sourceMap": true,
      "strict": true
  },
  "exclude": ["node_modules"]
}
